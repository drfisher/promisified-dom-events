{
	"compilerOptions": {
		"declaration": true,
		"diagnostics": true,
		"module": "es6",
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "./dist/",
		"removeComments": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"strictNullChecks": true,
		"target": "es5",
		"lib": [
			"es2015",
			"dom"
		]
	},
	"exclude": [
		"node_modules"
	]
}
